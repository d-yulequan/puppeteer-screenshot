    "commitizen": "^4.2.4",
    "cz-customizable": "6.7.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.0",
    "husky": "^8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "^2.7.1"
  },
  "config": {
